{
  "name": "schema-openapi",
  "version": "0.2.3",
  "description": "Pipeable API for OpenAPI specification and @effect/schemas compilers",
  "homepage": "https://github.com/sukovanej/schema-openapi",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "build": "tsup",
    "watch": "tsc -w",
    "format": "prettier -w src test",
    "check-format": "prettier --check src test",
    "version": "changeset version"
  },
  "keywords": [],
  "author": "Milan Suk <Milansuk@email.cz>",
  "license": "MIT",
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@changesets/cli": "^2.26.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/swagger-ui-express": "^4.1.3",
    "express": "^4.18.2",
    "prettier": "^2.8.8",
    "swagger-ui-express": "^4.6.3",
    "tsup": "^7.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@effect/data": "^0.12.7",
    "@effect/io": "^0.26.0",
    "@effect/schema": "^0.20.2"
  }
}
