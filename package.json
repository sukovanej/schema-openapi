{
  "name": "schema-openapi",
  "type": "module",
  "version": "0.27.0",
  "description": "Pipeable API for OpenAPI specification and @effect/schema compilers",
  "homepage": "https://github.com/sukovanej/schema-openapi",
  "main": "index.cjs",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": {
        "types": "./*.d.ts",
        "default": "./*.js"
      },
      "require": {
        "types": "./*.d.cts",
        "default": "./*.cjs"
      }
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "build": "tsup && cp package.json dist/ && cp readme.md dist/",
    "watch": "tsc -w",
    "format": "prettier -w src test",
    "check-format": "prettier --check src test",
    "version": "changeset version"
  },
  "keywords": [],
  "author": "Milan Suk <Milansuk@email.cz>",
  "license": "MIT",
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@changesets/cli": "^2.27.1",
    "@effect/schema": "^0.57.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/swagger-ui-express": "^4.1.6",
    "effect": "2.0.1",
    "express": "^4.18.2",
    "prettier": "^3.1.1",
    "swagger-ui-express": "^5.0.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.2"
  },
  "peerDependencies": {
    "@effect/schema": "^0.57.0",
    "effect": "^2.0.0"
  }
}
